<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">

	<bean id="guitar" class="com.zensar.spring.instruments.Guitar"></bean>
	<bean id="sinthesizer" class="com.zensar.spring.instruments.Sinthesizer"></bean>
	<bean id="tabla" class="com.zensar.spring.instruments.Tabla"></bean>
	<bean id="Susmita" class="com.zensar.spring.performers.Instrumentalist">
	<property name="instrumentSongMap">
	<map>
	
	<entry key-ref="tabla" value="songV"/>
	<entry key-ref="guitar" value="song3"/>
	<entry key-ref="sinthesizer" value="songN"/>
	
	
	</map>
	</property>
	
	
	</bean>
	
</beans>
